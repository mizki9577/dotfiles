"pulseaudio-ctl mute"
	XF86AudioMute

"pulseaudio-ctl up"
	XF86AudioRaiseVolume

"pulseaudio-ctl down"
	XF86AudioLowerVolume

"import -window root ~/pic/screenshot/$(date +%y%m%d%H%M%S).png"
	Print

"import -window $(xdotool getwindowfocus -f) ~/pic/screenshot/$(date +%y%m%d%H%M%S).png"
	Alt + Print
